{
  "name": "your-broccoli-plugin",
  "version": "0.0.1",
  "description": "A small description of your-broccoli-plugin",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && broccoli build dist",
    "build:serve": "broccoli serve",
    "test": "npm run build && mocha dist/tests --reporter tap",
    "test:lint": "npm run build && mocha dist/tests/lint.js",
    "test:debug": "npm run build && mocha debug dist/tests",
    "test:watch": "testem",
    "prepublish": "npm run build"
  },
  "keywords": [
    "broccoli",
    "plugin"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
  },
  "homepage": "",
  "files": [
    "dist/",
    "!dist/tests"
  ],
  "devDependencies": {
    "broccoli": "^0.16.9",
    "broccoli-babel-transpiler": "^5.6.0",
    "broccoli-cli": "^1.0.0",
    "broccoli-lint-eslint": "^2.7.0",
    "broccoli-merge-trees": "^1.1.4",
    "broccoli-stew": "^1.3.2",
    "broccoli-test-helper": "^1.0.0",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "mocha-eslint": "^2.1.1",
    "regenerator-runtime": "^0.9.5",
    "testem": "^1.13.0"
  },
  "dependencies": {
    "broccoli-plugin": "^1.2.1",
    "fs-tree-diff": "^0.5.2",
    "heimdalljs": "^0.2.1",
    "heimdalljs-logger": "^0.1.7"
  }
